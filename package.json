    "doc": "typedoc --readme typedoc.md --out docs/doc",
    "start": "npx rollup -c --watch",
    "prepare": "run-s build",
    "test": "jest",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "jest --watchAll",
